{
  "manifest_version": 2,
  "name": "Audio Orders",
  "description": "This extension plays audio when trades of a given size happen.",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["activeTab", "tabs", "https://www.bitfinex.com", "notifications"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["bundle.js"]
    }
  ],
  "icons": {
    "128": "icon128.png"
  }
}
